module Day02 exposing (finalInput, sampleInput, solution1, solution2)


solution1 : String -> Int
solution1 input =
    input
        |> String.trim
        |> String.split "\n"
        |> List.filterMap parseLineForProblem1
        |> List.map (\moves -> toPoints (toGameResult moves) moves.ourMove)
        |> List.sum


solution2 : String -> Int
solution2 input =
    input
        |> String.trim
        |> String.split "\n"
        |> List.filterMap parseLineForProblem2
        |> List.map (\moves -> toPoints (toGameResult moves) moves.ourMove)
        |> List.sum


toPoints : GameResult -> Move -> Int
toPoints gameResult move =
    gameResultToPoints gameResult + moveToPoints move


moveToPoints : Move -> Int
moveToPoints move =
    case move of
        Rock ->
            1

        Paper ->
            2

        Scissors ->
            3


gameResultToPoints : GameResult -> Int
gameResultToPoints gameResult =
    case gameResult of
        WeWin ->
            6

        Draw ->
            3

        WeLose ->
            0


toGameResult : { theirMove : Move, ourMove : Move } -> GameResult
toGameResult { theirMove, ourMove } =
    case ( theirMove, ourMove ) of
        ( Rock, Rock ) ->
            Draw

        ( Rock, Paper ) ->
            WeWin

        ( Rock, Scissors ) ->
            WeLose

        ( Paper, Rock ) ->
            WeLose

        ( Paper, Paper ) ->
            Draw

        ( Paper, Scissors ) ->
            WeWin

        ( Scissors, Rock ) ->
            WeWin

        ( Scissors, Paper ) ->
            WeLose

        ( Scissors, Scissors ) ->
            Draw


type GameResult
    = WeWin
    | Draw
    | WeLose


parseLineForProblem1 : String -> Maybe { theirMove : Move, ourMove : Move }
parseLineForProblem1 line =
    case String.split " " line of
        [ theirMoveString, ourMoveString ] ->
            Maybe.map2
                (\theirMove ourMove ->
                    { theirMove = theirMove
                    , ourMove = ourMove
                    }
                )
                (stringToMove theirMoveString)
                (stringToMove ourMoveString)

        _ ->
            Nothing


parseLineForProblem2 : String -> Maybe { theirMove : Move, ourMove : Move }
parseLineForProblem2 line =
    case String.split " " line of
        [ theirMoveString, expectedGameResultString ] ->
            Maybe.map2
                (\theirMove expectedGameResult ->
                    { theirMove = theirMove
                    , ourMove = expectedGameResultToOurMove theirMove expectedGameResult
                    }
                )
                (stringToMove theirMoveString)
                (stringToExpectedGameResult expectedGameResultString)

        _ ->
            Nothing


type Move
    = Rock
    | Paper
    | Scissors


expectedGameResultToOurMove : Move -> GameResult -> Move
expectedGameResultToOurMove theirMove expectedGameResult =
    case ( theirMove, expectedGameResult ) of
        ( Rock, WeWin ) ->
            Paper

        ( Rock, Draw ) ->
            Rock

        ( Rock, WeLose ) ->
            Scissors

        ( Paper, WeWin ) ->
            Scissors

        ( Paper, Draw ) ->
            Paper

        ( Paper, WeLose ) ->
            Rock

        ( Scissors, WeWin ) ->
            Rock

        ( Scissors, Draw ) ->
            Scissors

        ( Scissors, WeLose ) ->
            Paper


stringToExpectedGameResult : String -> Maybe GameResult
stringToExpectedGameResult string =
    case string of
        "X" ->
            Just WeLose

        "Y" ->
            Just Draw

        "Z" ->
            Just WeWin

        _ ->
            Nothing


stringToMove : String -> Maybe Move
stringToMove string =
    case string of
        "A" ->
            Just Rock

        "X" ->
            Just Rock

        "B" ->
            Just Paper

        "Y" ->
            Just Paper

        "C" ->
            Just Scissors

        "Z" ->
            Just Scissors

        _ ->
            Nothing


